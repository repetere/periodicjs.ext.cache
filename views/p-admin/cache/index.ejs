<% include ../../shared/header %>
<div class="ts-container">
	<section class="ts-text-container">

		<div class="ts-row ts-text-xs">
					
			<div class="ts-col-span6">
				<h3>Data Cache</h3>
				<div class="ts-document">
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Type:</strong></div><div class="ts-col-span4"><%- cache_config.dataCacheType %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Status:</strong></div><div class="ts-col-span4"><%- cache_config.data_cache_status %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Length:</strong></div><div class="ts-col-span4"><%- cache_size.data_cache_length %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Size:</strong></div><div class="ts-col-span4"><%- cache_size.data_cache_size %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span12 ts-text-center">
						<a class="ts-button-error-color ts-dialog-delete ts-button" data-href="/p-admin/cache/clear/data" data-deleted-redirect-href="/p-admin/cache">Clear Data Cache</a>
						</div>
					</div>
				</div>
			</div>
			<div class="ts-col-span6">
				<h3>View Cache</h3>
				<div class="ts-document">
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Type:</strong></div><div class="ts-col-span4"><%- cache_config.viewCacheType %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Status:</strong></div><div class="ts-col-span4"><%- cache_config.view_cache_status %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Length:</strong></div><div class="ts-col-span4"><%- cache_size.view_cache_length %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span7"><strong>Size:</strong></div><div class="ts-col-span4"><%- cache_size.view_cache_size %></div>
					</div>
					<hr>
					<div class="ts-row">
						<div class="ts-col-span12 ts-text-center">
						<a class="ts-button-error-color ts-dialog-delete ts-button" data-href="/p-admin/cache/clear/view" data-deleted-redirect-href="/p-admin/cache">Clear View Cache</a>
						</div>
					</div>
				</div>
			</div>

		</div>

		<div class="ts-container-margin-sm ts-text-center">
			<span class="ts-text-xx">
				Global Cache is <span class="ts-button"><%- cache_config.status %></span>
			</span>
		</div>
	</section>
</div>
<% include ../../shared/footer %>
